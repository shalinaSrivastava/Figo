        <activity android:name=".LoginDashboard" />
        <activity android:name=".Voucher.Expensemain" />
        <activity android:name=".Voucher.expenseentry" />
        <activity android:name=".Voucher.expensereport" />
        <activity android:name=".Voucher.loadmain" />
