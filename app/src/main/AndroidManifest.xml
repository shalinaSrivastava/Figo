        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5258083952445606~8114830990" />

        <activity android:name=".Loginactivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <!-- The API key for Google Maps-based APIs. -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".kioskstatus" />
        <activity android:name=".kioskadapter" />
        <activity android:name=".userdetail" />
        <activity android:name=".transactiondetails" />
        <activity android:name=".trackuser" />
        <activity android:name=".Walletuser" />
        <activity android:name=".walletuserdetailsadapter" />
        <activity android:name=".SimDetail" />
        <activity android:name=".simadapter" />

